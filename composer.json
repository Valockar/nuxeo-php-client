{
  "name": "valockar/nuxeo-php-client",
  "description": "PHP Client Library for the Nuxeo Automation API",
  "keywords": ["nuxeo", "automation", "client"],
  "homepage": "http://www.nuxeo.com",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Jonathan PIEC",
      "email": "valockar@gmail.com"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.3-dev"
    }
  },
  "require": {
    "php": "~7.2 || ~8.0 || ~8.1",
    "ext-json": "*",
    "guzzlehttp/guzzle": "~6.0 <= 6.5.5 || ~7.0 <= 7.8.1",
    "monolog/monolog": "~1.3 <= 1.27.0 || ~2.0, <= 2.5.0 || ~3.0, <= 3.5.0",
    "zbateson/mail-mime-parser": "~1.3 <= 1.3.3 || ~2.2 <= 2.2.3 || ~2.4, <= 2.4.0",
    "jms/serializer": "~2.0 <=2.3.0 || ~3.0 <= 3.30.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~8.5.26 || ~9.5.20 || ~10.5.13",
    "dms/phpunit-arraysubset-asserts": "~v0.5.0",
    "symfony/http-foundation": "~4.4.41 || ~6.4.4",
    "symfony/mime": "~4.4.41 || ~6.4.3",
    "enlightn/security-checker": "~1.11.0"
  },
  "autoload": {
    "psr-4": {
      "Nuxeo\\": "src/Nuxeo"
    }
  },
  "autoload-dev" : {
    "psr-4": {
      "Nuxeo\\Client\\Tests\\": "tests/Nuxeo/Client/Tests",
      "Nuxeo\\Client\\FTests\\": "ftests/Nuxeo/Client/FTests"
    }
  }
}
